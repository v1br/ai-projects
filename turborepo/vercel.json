{
    "version": 2,
    "builds": [
      {
        "src": "apps/web/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "apps/web/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  