{
  "name": "turborepo",
  "private": true,
  "scripts": {
    "build": "cd apps/web && bun run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.4.4",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "chart.js": "^4.4.8",
    "react-chartjs-2": "^5.3.0"
  }
}
